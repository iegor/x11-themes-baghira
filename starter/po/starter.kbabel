[poauxiliary]
Auxiliary=../../../de/messages/@DIR1@/@PACKAGE@.po
IgnoreFuzzy=true

[pocompendium]
CaseSensitive=false
Compendium=http://i18n.kde.org/po_overview/@LANG@.messages
IgnoreFuzzy=true
MatchContains=true
MatchEqual=true
MatchIsContained=false
MatchNGram=true
MatchWords=true
