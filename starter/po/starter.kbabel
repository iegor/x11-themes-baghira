[CatalogManager]
PoBaseDir=/cvs/baghira/starter/po
PotBaseDir=/cvs/baghira/starter/po
ValidationIgnoreURL=/home/thomas/.kde/share/apps/kbabel/spellignores

[Header]
AutoSyntaxCheck=false
Language=German

[KBabelDict]
ActiveModule=poauxiliary
ResultSplitter=50,283

[Project]
Name=starter

[Tags]
ArgExpressions=%[ndioxXucsfeEgGp],%([0-9]+(\\\\$))?[-+'#0]?[0-9]*(.[0-9]+)?[hlL]?[dioxXucsfeEgGp],%[0-9]+
